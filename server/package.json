{
  "name": "automedia-server",
  "version": "1.0.0",
  "description": "AutoMedia Backend API Server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:unit": "jest --testPathPattern=\"(unit|\\.unit\\.|\\.spec\\.)\"",
    "test:integration": "jest --testPathPattern=\"integration\"",
    "test:api": "jest --testPathPattern=\"api\"",
    "test:performance": "jest --testPathPattern=\"performance\"",
    "test:socket": "jest --testPathPattern=\"socket\"",
    "test:security": "jest --testPathPattern=\"auth|security\"",
    "test:staged": "jest --bail --findRelatedTests",
    "test:reset-db": "jest --setupFilesAfterEnv=./src/__tests__/setup.ts --forceExit",
    "test:clear-cache": "jest --clearCache",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.4.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "openai": "^4.0.0",
    "redis": "^4.6.7",
    "socket.io": "^4.7.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "twitter-api-v2": "^1.15.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongodb-memory-server": "^1.8.0",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.5",
    "@types/node-cron": "^3.0.11",
    "@types/socket.io-client": "^1.4.36",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "mongodb-memory-server": "^10.2.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "redis-mock": "^0.56.3",
    "socket.io-client": "^4.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6"
  }
}
